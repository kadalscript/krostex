<%= form_for(@a_kemasan) do |f| %>
  <table class="table-bordered" align="center" width="60%">
    <tr class="titleJudulTable"><th colspan="3"><%= @form_title %></th></tr>
    <%= render partial: COMMON_ERRORS, locals: { object: @a_kemasan } if @a_kemasan.errors.any? %>
    <%= render partial: "navigator" unless (action_name == 'new' or action_name == 'edit' or action_name == 'destroy_show') %>
    <%= render partial: "columns", locals: { f: f, attributes: @attributes } %>
    <tr>
      <th colspan="3" class="titleJudulTable" width="100%">
        <%= f.submit SAVE, class: :btn, confirm: "Simpan data?" unless ["destroy_show", "show"].include?(action_name)  %>
        <%= link_to DELETE, a_kemasan_path(@a_kemasan), method: :delete, class: :btn, confirm: "#{DELETE} data?" if action_name.eql?('destroy_show') %>
        <%= link_to CLOSE, "javascript:void(0)", onclick: "closingForm()", class: :btn %>
      </th>
    </tr>
  </table>
<% end %>
<%= hidden_value(a_kemasans_path) %>

<script type="text/javascript">
   <% if (action_name.eql? 'edit') %>
          $("#a_badan_usaha_kode").attr("readonly","true")
   <% end %>

</script>
