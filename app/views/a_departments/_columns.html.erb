<% ADepartment.column_names.each do |column_name| %>
  <% unless @hidden_columns.include?(column_name) %>
    <% if !(["new", "create"].include?(action_name) && ["updated_at", "updated_by"].include?(column_name)) %>
      <%= raw(showing_row(ADepartment, column_name, attributes, f, @a_department)) %>
    <% end %>
  <% end %>
<% end %>
