<% if @a_provinsis.blank? %>
  $('#a_city_id_provinsi').html('<%= escape_javascript(select_tag("id_provinsi", options_for_select(""), prompt: "- Provinsi -")) %>')
<% else %>
$('#a_city_id_provinsi').html('<%= escape_javascript(select_tag("id_provinsi", options_for_select(@a_provinsis.collect {|provinsi| [ provinsi.nama, provinsi.kode ] }), prompt: "- Provinsi -")) %>')

<% end %>
