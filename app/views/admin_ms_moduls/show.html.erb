
<table class="table-bordered" align="center" width="60%">
  <tr class="titleJudulTable">
    <th colspan="3">
      FORM ADMIN MS MODUL
    </th>
  </tr>
  <tr>
    <th colspan="3">
      <%= link_to 'Tambah', new_admin_ms_modul_path %> |
      <% if @admin_ms_modul.id %>
      <%= link_to 'Koreksi', edit_admin_ms_modul_path(@admin_ms_modul) %> |
      <% end %>
      <%= link_to 'Hapus', destroy_show_admin_ms_modul_path(@admin_ms_modul.id) %> |
    </th>
  </tr> 
  <tr>
    <th align="left">&nbsp;Id_modul</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= text_field_tag "", @admin_ms_modul.id_modul, :style => "width:300px", :readonly=>"true", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Keterangan</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= text_field_tag "", @admin_ms_modul.keterangan, :style => "width:300px", :readonly=>"true", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Status1</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= text_field_tag "", @admin_ms_modul.status1, :style => "width:300px", :readonly=>"true", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Nama URL</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= text_field_tag "", @admin_ms_modul.nm_url, :style => "width:300px", :readonly=>"true", class: :required %>
    </th> 
  </tr>
  <tr>
    <th colspan="3" class="titleJudulTable" width="100%">
      <%= link_to "Tutup", "#", onclick: "is_tutup()", class: :btn %>
    </th>
  </tr>
</table>


<input type="hidden" id="url_back" name="admin_ms_modul_back" value="<%= admin_ms_moduls_path %>">

<%= render partial: "admin_ms_moduls/detail_table" %>
