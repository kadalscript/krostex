<%= form_for(@admin_ms_modul) do |f| %>
<table class="table-bordered" align="center" width="60%">
  <tr class="titleJudulTable">
    <th colspan="3">
      <%= title %>
    </th>
  </tr>
  <% render template: "/shared/errors", errors: @admin_ms_modul.errors if @admin_ms_modul.errors.any? %>
  <tr>
    <th align="left">&nbsp;Id_modul</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :id_modul, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Keterangan</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :keterangan, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Modulname</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :modulname, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Appname</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :appname, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Status1</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :status1, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Buttongroup</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :buttongroup, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Nama URL</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :nm_url, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Nama URL 1</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :nm_url1, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Nama URL 2</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :nm_url2, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Kode User</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :kduser, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th align="left">&nbsp;Tipe Modul</th>  
    <th align="center"> :</th>
    <th align="left"> 
      <%= f.text_field :tipemodul, :style => "width:300px", class: :required %>
    </th> 
  </tr>
  <tr>
    <th colspan="3" class="titleJudulTable" width="100%">
      <%= f.submit "Simpan", class: :btn, onclick: "isSimpan()" unless action_name.eql?('destroy_show')  %>
      <%= link_to "Hapus", admin_ms_modul_path(@admin_ms_modul.id), method: :delete, class: :btn, confirm: "Hapus data?" if action_name.eql?('destroy_show') %>
      <%= link_to "Tutup", "#", onclick: "is_tutup()", class: :btn %>
    </th>
  </tr>
</table>
<% end %>

<input type="hidden" id="url_back" name="admin_ms_modul_back" value="<%= admin_ms_moduls_path %>">
