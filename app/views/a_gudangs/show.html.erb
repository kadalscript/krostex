<%= form_for(@a_gudang) do |f| %>
<table class="table-bordered" align="center" width="60%">
  <tr class="titleJudulTable">
    <th colspan="3">
      FORM DATA GUDANG 
    </th>
  </tr>     
  <tr>
    <th colspan="3">
     <%= link_to 'Tambah' , new_a_gudang_path  %> |
     <% if @a_gudang.id %>
     <%= link_to 'Koreksi', edit_a_gudang_path(@a_gudang) %> |
     <% end %>
     <%= link_to 'Hapus'  , :controller => a_gudangs_path, :action =>"destroy_show", :id => @a_gudang.id %> |
   </th> 
 </tr>
 <tr>
  <th align="left">
   &nbsp;Kode
 </th>  
 <th align="center"> 
   :
 </th>
 <th align="left"> 
   <%= f.text_field :simbol, { :style => "width:300px", :readonly=>"true" } %>
 </th> 
</tr>
<tr>
  <th align="left">
    &nbsp;Nama Gudang
  </th>  
  <th align="center"> 
    :
  </th>
  <th align="left"> 
   <%= f.text_field :nama, { :style => "width:300px", :readonly=>"true" } %>
 </th> 
</tr>  
<tr>
 <th align="left">
   &nbsp;Status
 </th>  
 <th align="center"> 
   :
 </th>
 <th align="left"> 
   <%= f.text_field :st_progress, { :style => "width:300px", :readonly=>"true" } %>
 </th> 
</tr>  
<tr>
 <th align="left">
   &nbsp;Opr Input
 </th>  
 <th align="center"> 
   :
 </th>
 <th align="left"> 
   <%= f.text_field :update_by, { :style => "width:300px", :readonly=>"true" } %>
 </th> 
</tr>  
<tr>
 <th colspan="3" class="titleJudulTable" width="100%">

   <input type="button" id="a_gudang_btn_back" name="a_gudang_btn_back" value="Tutup" onclick="closingForm()" class="btn">
 </th>
</tr>
</table>      
<% end %>

<%= hidden_value(a_gudangs_path(page: params[:page])) %>
