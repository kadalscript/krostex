<%= form_for(@a_negara) do |f| %>
  <table class="table-bordered" align="center" width="60%">
    <tr class="titleJudulTable"><th colspan="3"><%= @form_title %></th></tr>
    <%= render partial: COMMON_ERRORS, locals: { object: @a_negara } if @a_negara.errors.any? %>
    <%= render partial: "navigator" unless ['new','edit','destroy_show'].include?(action_name) %>
    <%= render partial: "columns", locals: { f: f, attributes: @attributes } %>
    <tr>
      <th colspan="3" class="titleJudulTable" width="100%">
        <%= f.submit SAVE, class: :btn, confirm: "Simpan Data?" unless ["show", "destroy_show"].include?(action_name)  %>
        <%= link_to DELETE, a_negara_path(@a_negara), method: :delete, class: :btn, confirm: "#{DELETE} data?" if action_name.eql?('destroy_show') %>
        <%= link_to CLOSE, "javascript:void(0)", onclick: "closingForm()", class: :btn %>
      </th>
    </tr>
  </table>
<% end %>
<%= hidden_value(a_negaras_path) %>

<script>
   <% !unless ['new'].include?(action_name) %>
      $("#a_negara_kode").attr("readonly","true");
   <% end %>
</script>