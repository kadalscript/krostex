<% ABadanUsaha.column_names.each do |column_name| %>
  <% unless @hidden_columns.include?(column_name) %>
    <tr>
      <th align="left">&nbsp;<%= ABadanUsaha.human_attribute_name(column_name).titleize %></th>
      <th align="center">:</th>
      <th align="left"><%= f.text_field column_name, attributes %></th>
    </tr>
  <% end %>
<% end %>
