<% ASatuan.column_names.each do |column_name| %>
  <% unless ["id", "created_at", "updated_at", "updated_by"].include?(column_name) %>
    <tr>
      <th align="left">&nbsp;<%= ASatuan.human_attribute_name(column_name).titleize %></th>
      <th align="center">:</th>
      <th align="left"><%= f.text_field column_name, attributes %></th>
    </tr>
  <% end %>
<% end %>
