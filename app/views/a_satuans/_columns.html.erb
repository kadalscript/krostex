<% ASatuan.column_names.each do |column_name| %>
  <% unless ["id", "created_at", "updated_by", "updated_at"].include?(column_name) %>
    <tr>
      <th align="left">&nbsp;<%= ASatuan.human_attribute_name(column_name).titleize %></th>
      <th align="center">:</th>
      <th align="left">
        <%= f.text_field column_name, attributes %>
      </th>
    </tr>
  <% end %>
<% end %>

<% unless ["new", "create"].include?(action_name) %>
  <tr>
    <th align="left">&nbsp;<%= ASatuan.human_attribute_name("updated_by").titleize %></th>
    <th align="center">:</th>
    <th align="left">
      <%= f.text_field :updated_by, @updated_at_attributes %>
    </th>
  </tr>
  <tr>
    <th align="left">&nbsp;<%= ASatuan.human_attribute_name("updated_at").titleize %></th>
    <th align="center">:</th>
    <th align="left">
      <%= text_field_tag "updated_at", @a_satuan.updated_at.strftime("%a %b %d, %H:%M %p"), @updated_at_attributes %>
    </th>
  </tr>
<% end %>