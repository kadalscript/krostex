<% ASatuan.column_names.each do |column_name| %>
  <% unless ["id", "created_at", "updated_by"].include?(column_name) %>
    <tr>
      <th align="left">&nbsp;<%= ASatuan.human_attribute_name(column_name).titleize %></th>
      <th align="center">:</th>
      <th align="left">
        <% if column_name == 'updated_at' %>
          <% unless ["new", "create"].include?(action_name) %>
            <%= text_field_tag column_name, @a_satuan.updated_at.strftime("%a %b %d, %H:%M %p"), @updated_at_attributes %>
          <% else %>
            <%= text_field_tag "dummy_date", Time.now.strftime("%a %b %d, %H:%M %p"), @updated_at_attributes %>
          <% end %>
        <% else %>
          <%= f.text_field column_name, attributes %>
        <% end %>
      </th>
    </tr>
  <% end %>
<% end %>
