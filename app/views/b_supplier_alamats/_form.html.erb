<!-- Modal -->
<div id="formSupplierModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <%= form_for object, url: b_supplier_b_supplier_alamats_path(@b_supplier), method: :post, html: { class: 'form-horizontal' } do |f| %>
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
      <h3 id="myModalLabel"><%= title %></h3>
    </div>
    <div class="modal-body">
      <% BSupplierAlamat.column_names.each do |column_name| %>
        <% unless ["id", "created_at", "updated_at", "b_supplier_id"].include?(column_name) %>
          <div class="control-group">
            <%= f.label column_name, column_name.gsub('_', ' ').titleize, class: 'control-label' %>
            <div class="controls">
              <%= f.text_field column_name %>
            </div>
          </div>
        <% end %>
      <% end %>
    </div>
    <div class="modal-footer">
      <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
      <button class="btn btn-primary">Create</button>
    </div>
  <% end %>
</div>
