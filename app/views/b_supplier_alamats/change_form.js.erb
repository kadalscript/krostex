@action_var      = (params[:type] == 'koreksi' ? 'update' : 'destroy')
@action_type_var = (params[:type] == 'koreksi' ? 'Update' : 'Delete')
%>

$('#supplierModal').html("<%= escape_javascript(render partial: 'b_supplier_alamats/form', locals: { title: @judul_var, b_supplier_alamat: @b_supplier_alamat, controller: 'b_supplier_alamats', action: @action_var, b_supplier: @b_supplier, action_type: @action_type_var })%>")

$('#formSupplierModal').modal('show');