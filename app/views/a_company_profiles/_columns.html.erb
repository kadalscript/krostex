<% ACompanyProfile.column_names.each do |column_name| %>
  <% unless ["id", "created_at", "updated_at"].include?(column_name) %>
    <tr>
      <th align="left">
        &nbsp;
        <% if ["alamat_title", "alamat_01", "alamat_02", "alamat_03"].include?(column_name) %>
          <%= column_name.gsub('_title', " ").titleize if ["alamat_title"].include?(column_name) %>
          <span style="<%= "margin-left: 53px;" unless ["alamat_title"].include?(column_name) %>"><%= column_name.gsub('alamat_', "").titleize %></span>
        <% else %>
          <%= company_profile_column(column_name).titleize %>
        <% end %>
      </th>
      <th align="center">:</th>
      <th align="left">
        <%= f.text_field column_name, attribute %>
      </th>
    </tr>
  <% end %>
<% end %>
