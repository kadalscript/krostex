<% ACompanyProfile.column_names.each do |column_name| %>
  <% unless ["id", "created_at", "updated_at", "kode"].include?(column_name) %>
    <tr>
      <th align="left">&nbsp; <%= ACompanyProfile.human_attribute_name(column_name).titleize %></th>
      <th align="center">:</th>
      <th align="left">
        <%= f.text_field column_name, ["homepage"].include?(column_name) ? (attributes.tap { |attribute| attribute.delete(:class) }) : attributes %>
      </th>
    </tr>
  <% end %>
<% end %>
